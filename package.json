{
  "name": "cra-ssr",
  "homepage": "https://cra-ssr.herokuapp.com",
  "version": "0.1.0",
  "private": true,
  "description": "Boilerplate for cra with ts and ssr capability",
  "main": "src/index.tsx",
  "repository": "https://github.com/chasepoirier/cra-ssr.git",
  "author": "Chase Poirier <chase.n.poirier@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "cross-env NODE_ENV=production node ./dist_server/index.js",
    "server": "yarn compile-server && cross-env NODE_ENV=development nodemon ./dist_server/",
    "dev": "concurrently --kill-others \"yarn start\" \"yarn server\"",
    "compile-server": "rm -rf ./dist && ./node_modules/.bin/babel -d ./dist_server ./server -s --copy-files --extensions '.ts','.tsx','.js'",
    "compile-client": "rm -rf ./dist && ./node_modules/.bin/babel -d ./dist_client ./src -s --copy-files --extensions '.ts','.tsx','.js'",
    "compile": "yarn compile-server && yarn compile-client && yarn build",
    "postinstall": "yarn compile"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-typescript": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connected-react-router": "^4.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "forcedomain": "^1.0.0",
    "history": "^4.7.2",
    "husky": "^1.1.0",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-render-html": "^0.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "concurrently": "^4.1.0"
  }
}